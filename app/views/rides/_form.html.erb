<!-- views/rides/_form.html.erb -->
<div class="container">
<%= form_for @ride do |f| %>
  <div class="form-group">
    <%= f.label :start_address %>
    <%= f.select :start_address, [['McPherson County', [-101.244478, 41.487089].to_json],
        ['Velly County', [-98.893404, 41.404738].to_json],['McPherson park', [-101.244478, 41.487089].to_json], ['County Park', [-101.244478, 41.487089].to_json]]%>
  </div>

  <div class="form-group">
    <%= f.label :destination_address %>
    <%= f.select :destination_address, [['McPherson County', [-101.244478, 41.487089].to_json],
        ['Velly County', [-98.893404, 41.404738].to_json],['McPherson park', [-101.244478, 41.487089].to_json], ['County Park', [-101.244478, 41.487089].to_json]]%>
  </div>

  <%= f.submit class: "btn btn-primary" %>
<% end %>
</div>
